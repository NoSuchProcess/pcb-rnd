# this must run after the src/Makefile.in because it depends on:
# /local/pcb/polyclip

include [@@/local/pcb/librnd_template@/debug.tmpasm@]

print [@### Autogenerated by ./configure, DO NOT EDIT ###

ROOT=../..
PLUGDIR=$(ROOT)/src_plugins

LIBRND_SHAREDIR=@/local/pcb/librnd_prefix@/share/librnd4
include $(LIBRND_SHAREDIR)/librnd.mak

PLUG=$(PLUGDIR)/io_bxl
CFLAGS = @/local/pcb/c89flags@ @/local/rnd/CFLAGS@ @?/local/pcb/librnd_extra_inc@ -I$(PLUG) -I$(ROOT) -I$(ROOT)/src -I$(LIBRND_INCDIR) -I$(ROOT)/src_3rd -I@/local/pcb/librnd_prefix@/include/librnd4/librnd/src_3rd
LDFLAGS = @/local/rnd/LDFLAGS@ @/local/pcb/polyclip@ -lrnd-core -lrnd-3rd
CC=@cc/cc@


all: rect-union-rnd$(LIBRND_EXE)

include ../../Makefile.conf

rect-union-rnd$(LIBRND_EXE): rect-union-rnd.o
	$(CC) -o rect-union-rnd$(LIBRND_EXE) $(LDFLAGS) rect-union-rnd.o

rect-union-rnd.o: rect-union-rnd.c
	$(CC) -c $(CFLAGS) rect-union-rnd.c -o rect-union-rnd.o

install_all:
	$(SCCBOX) mkdir -p "$(BINDIR)"
	$(SCCBOX) $(HOW) "rect-union-rnd$(LIBRND_EXE)" "$(BINDIR)/rect-union-rnd$(LIBRND_EXE)"

install:
	$(MAKE) install_all HOW="install"

linstall:
	$(MAKE) install_all HOW="linstall -f"

uninstall:
	$(MAKE) install_all HOW="uninstall"

clean:
	$(SCCBOX) rm -f -q  rect-union-rnd$(LIBRND_EXE) rect-union-rnd.o

distclean: clean
	$(SCCBOX) rm -f -q Makefile

@]

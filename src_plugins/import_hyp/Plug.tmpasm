put /local/pcb/mod {import_hyp}
append /local/pcb/mod/OBJS [@ $(PLUGDIR)/import_hyp/hyp.o $(PLUGDIR)/import_hyp/import_hyp.o @]
put /local/pcb/mod/YACC {$(PLUGDIR)/import_hyp/.y}
put /local/pcb/mod/LEX  {$(PLUGDIR)/import_hyp/.l}


switch /local/pcb/import_hyp/controls
	case {buildin}   include /local/pcb/tmpasm/buildin; end;
	case {plugin}    include /local/pcb/tmpasm/plugin; end;
	case {disable}   include /local/pcb/tmpasm/disable; end;
end

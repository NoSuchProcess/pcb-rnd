put /local/rnd/mod {shape}
put /local/rnd/mod/MENUFILE {shape-menu.lht}
put /local/rnd/mod/MENUVAR {shape_menu}
append /local/rnd/mod/OBJS [@
 $(PLUGDIR)/shape/shape.o
@]


append /local/pcb/stroke/enable {}
append /local/pcb/stroke/buildin {}

put /local/pcb/mod {stroke}
put /local/pcb/mod/OBJS [@ $(PLUGDIR)/stroke/stroke.o @]

if /local/pcb/stroke/enable then
	append /local/pcb/LDFLAGS {-lstroke}
	if /local/pcb/stroke/buildin then
		include {Makefile.in.mod/Buildin}
	else
		include {Makefile.in.mod/Plugin}
	end
else
	include {Makefile.in.mod/Disable}
end




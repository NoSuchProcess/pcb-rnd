put /local/pcb/mod {legacy_func}
put /local/pcb/mod/OBJS [@ $(PLUGDIR)/legacy_func/legacy_func.o @]

switch /local/pcb/legacy_func/controls
	case {buildin}   include {Makefile.in.mod/Buildin}; end;
	case {plugin}    include {Makefile.in.mod/Plugin}; end;
	case {disable}   include {Makefile.in.mod/Disable}; end;
end

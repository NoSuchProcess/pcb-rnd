append /local/pcb/legacy_func/enable {}
append /local/pcb/legacy_func/buildin {}

put /local/pcb/mod {legacy_func}
put /local/pcb/mod/OBJS [@ $(PLUGDIR)/legacy_func/legacy_func.o @]

if /local/pcb/legacy_func/enable then
	if /local/pcb/legacy_func/buildin then
		include {Makefile.in.mod/Buildin}
	else
		include {Makefile.in.mod/Plugin}
	end
else
	include {Makefile.in.mod/Disable}
end




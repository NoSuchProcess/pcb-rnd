put /local/pcb/mod {export_lpr}
put /local/pcb/mod/OBJS [@ $(PLUGDIR)/export_lpr/lpr.o @]

switch /local/pcb/export_lpr/controls
	case {buildin}   include {Makefile.in.mod/Buildin}; end;

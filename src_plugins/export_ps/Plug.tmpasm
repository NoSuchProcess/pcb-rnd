append /local/pcb/export_ps/enable {}
append /local/pcb/export_ps/buildin {}

put /local/pcb/mod {export_ps}
put /local/pcb/mod/OBJS [@ $(PLUGDIR)/export_ps/ps.o  $(PLUGDIR)/export_ps/eps.o  @]

if /local/pcb/export_ps/enable then
	if /local/pcb/export_ps/buildin then
		include {Makefile.in.mod/Buildin}
	else
		include {Makefile.in.mod/Plugin}
	end
else
	include {Makefile.in.mod/Disable}
end

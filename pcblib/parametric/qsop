#!/bin/sh

# Reference: Microchip Packaging Specification DS00000049BX (en012702.pdf), QSOP

#@@example qsop(14)

#@@purpose Generate QSOP packages.

#@@desc Generate QSOP packages with variable number of pins and
#@@desc row spacing

#@@include so
#@@include common.awk

defaults='
?pad_spacing=25mil,
?row_spacing=213mil,
?int_bloat=22mil,
?ext_bloat=23mil,
?rarc=25,
?silk_ext_x=18,
?silk_ext_y=22,
?pad_thickness=16mil
'

awk -f `dirname $0`/common.awk -f `dirname $0`/so.awk -v "args=$defaults,$*" -v gen=`basename $0` -v "genfull=$0"


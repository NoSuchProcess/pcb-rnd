###append /local/pcb/HIDS  png###
###append /local/pcb/CFLAGS  -I./hid/png  ###
###nappend /local/pcb/LIBS libs/gui/gd/cflags###
###append /local/pcb/OBJS  hid/png/png.o###
###append /local/pcb/LIBS  ###
###nappend /local/pcb/LIBS libs/gui/gd/ldflags###

###append /local/pcb/RULES  

LIBPNG_SRCS=
hid/png/png_lists.h : ${LIBPNG_SRCS} Makefile
	$(MKDIR_P) hid/png
	true > $@
	(for f in ${LIBPNG_SRCS} ; do cat $$f ; done) | grep "^REGISTER" > $@.tmp
	mv $@.tmp $@


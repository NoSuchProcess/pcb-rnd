###append /local/pcb/CFLAGS  -I./hid/ps###
###append /local/pcb/OBJS  
	hid/ps/ps.o
	hid/ps/eps.o
###


###append /local/pcb/RULES  
LIBPS_SRCS =
hid/ps/ps_lists.h : ${LIBPS_SRCS} Makefile
	$(MKDIR_P) hid/ps
	true > $@
	(for f in ${LIBPS_SRCS} ; do cat $(srcdir)/$$f ; done) | grep "^REGISTER" > $@.tmp
	mv $@.tmp $@
###

###append /local/pcb/HIDS  gcode###
###append /local/pcb/CFLAGS  -I./hid/gcode###
###append /local/pcb/OBJS  
	hid/gcode/gcode.o
	hid/gcode/decompose.o
	hid/gcode/trace.o
	hid/gcode/curve.o
###

###append /local/pcb/RULES  

LIBGCODE_SRCS = hid/gcode/gcode_lists.h
hid/gcode/gcode_lists.h : ${LIBGCODE_SRCS} Makefile
	$(MKDIR_P) hid/gcode
	true > $@
	(for f in ${LIBGCODE_SRCS} ; do cat $$f ; done) | grep "^REGISTER" > $@.tmp
	mv $@.tmp $@

###

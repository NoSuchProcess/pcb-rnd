###append /local/pcb/CFLAGS  -I./hid/nelma###
###append /local/pcb/OBJS  hid/nelma/nelma.o###

###append /local/pcb/RULES 
LIBNELMA_SRCS = 
hid/nelma/nelma_lists.h : ${LIBNELMA_SRCS} Makefile
	$(MKDIR_P) hid/nelma
	true > $@
	(for f in ${LIBNELMA_SRCS} ; do cat $$f ; done) | grep "^REGISTER" > $@.tmp
	mv $@.tmp $@
###

